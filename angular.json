{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "angular-library": {
      "root": "",
      "sourceRoot": "",
      "projectType": "library",
      "prefix": "lib",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "tsconfig.lib.json",
            "project": "ng-package.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {}
        }
      }
    },
    "library": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "library",
      "sourceRoot": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/angular-library",
            "index": "library/src/index.html",
            "main": "library/src/main.ts",
            "polyfills": "library/src/polyfills.ts",
            "tsConfig": "library/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "library/src/favicon.ico",
              "library/src/assets",
              "library/src/.htaccess",
              "library/src/documentation.json",
              "library/config",
              "library/assets"
            ],
            "styles": [
              "library/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2000kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "40kb",
                  "maximumError": "50kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "library/src/environments/environment.ts",
                  "with": "library/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "library:build:production"
            },
            "development": {
              "buildTarget": "library:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "library:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "library/src/test.ts",
            "polyfills": "library/src/polyfills.ts",
            "tsConfig": "library/tsconfig.spec.json",
            "karmaConfig": "library/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "library/src/favicon.ico",
              "library/src/assets"
            ],
            "styles": [
              "library/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html",
              "library/**/*.ts",
              "library/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": "5e1027be-5a48-4d77-af80-47c70f3e157c"
  }
}
